<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch=".."
           review="https://android-review.googlesource.com/" />
  <default revision="studio-master-dev"
           remote="aosp"
           sync-j="4" />

  <!-- BEGIN open-source projects -->
  <project path="tools/buildSrc" name="platform/tools/buildSrc">
    <linkfile src="base/build.xml"         dest="tools/build.xml" />
    <linkfile src="base/build.gradle"      dest="tools/build.gradle" />
    <copyfile src="base/settings.gradle"   dest="tools/settings.gradle" />
    <copyfile src="base/gradlew"           dest="tools/gradlew" />
    <copyfile src="base/gradlew.bat"       dest="tools/gradlew.bat" />
    <linkfile src="base/gradle.properties" dest="tools/gradle.properties" />
  </project>

  <project path="external/dexter" name="platform/tools/dexter" />
