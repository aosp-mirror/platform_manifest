<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- This manifest can be used for local development, as a convenient way of
       checking out all the projects needed to build NDK components that are
       built using Kokoro. The http://ab autobuilder doesn't use this manifest,
       and Kokoro itself has its own job config files in google3 for specifying
       which repositories to check out. -->

  <remote  name="aosp"
           fetch=".."
           review="https://android-review.googlesource.com/" />
  <default revision="master"
           remote="aosp"
           sync-j="4" />

  <superproject name="platform/superproject" remote="aosp" revision="ndk-kokoro-main" />

  <project path="external/android-cmake" name="platform/external/android-cmake" revision="cmake-master-dev" />
  <project path="external/bzip2" name="platform/external/bzip2" />
  <project path="external/cmake" name="platform/external/cmake" revision="cmake-master-dev" />
  <project path="external/libffi" name="platform/external/libffi" />
  <project path="external/ninja" name="platform/external/ninja" revision="cmake-master-dev" />
  <project path="external/python/cpython3" name="platform/external/python/cpython3" />
  <project path="external/zlib" name="platform/external/zlib" />
  <project path="ndk" name="platform/ndk" />
  <project path="prebuilts/cmake/darwin-x86" name="platform/prebuilts/cmake/darwin-x86" />
  <project path="prebuilts/cmake/linux-x86" name="platform/prebuilts/cmake/linux-x86" />
  <project path="prebuilts/cmake/windows-x86" name="platform/prebuilts/cmake/windows-x86" />
  <project path="prebuilts/ninja/darwin-x86" name="platform/prebuilts/ninja/darwin-x86" revision="cmake-master-dev" />
  <project path="prebuilts/ninja/linux-x86" name="platform/prebuilts/ninja/linux-x86" revision="cmake-master-dev" />
  <project path="prebuilts/ninja/windows-x86" name="platform/prebuilts/ninja/windows-x86" revision="cmake-master-dev" />
  <project path="prebuilts/python/darwin-x86" name="platform/prebuilts/python/darwin-x86" revision="llvm-toolchain" />
  <project path="prebuilts/python/linux-x86" name="platform/prebuilts/python/linux-x86" revision="llvm-toolchain" />
  <project path="prebuilts/python/windows-x86" name="platform/prebuilts/python/windows-x86" revision="llvm-toolchain" />
  <project path="toolchain/make" name="toolchain/make" />
  <project path="toolchain/ndk-kokoro" name="toolchain/ndk-kokoro" />
  <project path="toolchain/xz" name="toolchain/xz" />
  <project path="tools/ndkports" name="platform/tools/ndkports" />
</manifest>
